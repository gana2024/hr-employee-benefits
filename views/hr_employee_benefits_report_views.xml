<odoo>
    <record id="view_hr_employee_benefits_report_tree" model="ir.ui.view">
        <field name="name">hr.employee.benefits.report.tree</field>
        <field name="model">hr.employee.benefits.report</field>
        <field name="arch" type="xml">
            <tree string="Employee Benefits Report">
                <field name="employee_id"/>
                <field name="benefit_type"/>
                <field name="amount"/>
                <field name="start_date"/>
                <field name="end_date"/>
            </tree>
        </field>
    </record>

    <record id="action_hr_employee_benefits_report" model="ir.actions.act_window">
        <field name="name">Employee Benefits Report</field>
        <field name="res_model">hr.employee.benefits.report</field>
        <field name="view_mode">tree,pivot,graph</field>
        <field name="view_id" ref="view_hr_employee_benefits_report_tree"/>
    </record>


</odoo>